syntax = "v1"
type (
    tagGetReq {
        Name string `form:"name,optional"`
    }
)

@server(
    group: admin/tag
    middleware: AuthMiddleware
    prefix: api/v1/admin/tag
)

service portal {
    @handler Get
    get / (tagGetReq) returns (Response)

    @handler Delete
    delete /:id (oneReq) returns (Response)
}